.find_main_top_searchbar {
  position: absolute;
  width: 100%;
  height: 40px;
  top: 0px;
  left: 0px;
  background-color: #ffffff;
}

.find_main_top_searchbar_area {
  position: absolute;
  top: 3px;
  height: 32px;
  left: 10px;
  right: 10px;
  background-color: #ffffff;
  border-radius: 5px;
  border: 1pt solid #ccc;
}

.find_main_top_searchbar_area_search_icon {
  position: absolute;
  top: 3px;
  right: 3px;
}

.find_main_top_searchbar_area_input {
  width: 100%;
  height: 100%;
  font-size: 13px;
  text-indent: 3px;
  color: #333333;
  background-color: #ffffff;
}

.find_main_top_image {
  position: absolute;
  width: 100%;
  height: 100px;
  top: 20px;
  left: 0px;  
}

.find_main_top_image_img {
  background-color: #eeeeee;
}