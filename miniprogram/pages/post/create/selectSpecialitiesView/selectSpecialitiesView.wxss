.wrapper {
  background-color: #33333345;
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1000;
}

.selectSpecialitiesView {
  background-color: #fff;
  height: 245px;
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
}

.selectSpecialitiesViewBtnCancel {
  background-color: #fff;
  height: 30px;
  width: 70px;
  color: #666;
  font-size: 14px;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
}

.selectSpecialitiesViewBtnConfirm {
  background-color: #fff;
  height: 30px;
  width: 70px;
  color: #26bb2e;
  font-size: 14px;
  text-align: center;
  display: inline-block;
  position: absolute;
  top: 0;
  right: 0;
}

.selectSpecialitiesTableview {
  background-color: #0ff;
  position: absolute;
  top: 30px;
  left: 0;
  right: 0;
  bottom: 0;
}

.selectSpecialitiesItemCell {
  background-color: #fff;
  color: #333;
  font-size: 15px;
  height: 35px;
  width: 100%;
  position: relative;
  top: 0;
}